{
  "name": "mauretto78/locker-manager",
  "type": "library",
  "description": "Locker Manager",
  "keywords": ["lock", "php"],
  "homepage": "https://github.com/mauretto78/locker-manager",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/mauretto78/locker-manager/issues"
  },
  "authors": [
    {
      "name": "Mauro Cassani",
      "email": "assistenza@easy-grafica.com",
      "homepage": "https://github.com/mauretto78"
    }

  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "friendsofphp/php-cs-fixer": "^2.3",
    "satooshi/php-coveralls": "dev-master",
    "predis/predis": "^1.1"
  },
  "suggest": {
    "predis/predis": "Flexible and feature-complete Redis client.",
    "ext-curl": "Allows access to Webdis when paired with phpiredis"
  }
